#include<bits/stdc++.h>
using namespace std;
int main()
{ 
  int n[10] = {1,2,3};
  int i = 2;
  cout<< i[n]<<endl;//different type 

  cout<<"first block address "<<n<<endl;
  cout<<"second block address"<<n+1<<endl;//just moved to next memeory block 

  int arr1[10] =  {1,7,3};
  cout<<*arr1+1<<endl;
  cout<<*(arr1 + 1)<<endl; ;
  cout<<*(arr1) + 1;
  return 0; 


}
